{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/data",
      "dest": "src/server.ts"
    },
    {
      "src": "/echo/(.*)",
      "dest": "src/server.ts"
    },
    {
      "src": "/add/(.*)",
      "dest": "src/server.ts"
    },
    {
      "src": "/view/(.*)",
      "dest": "src/server.ts"
    },
    {
      "src": "/clear",
      "dest": "src/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ]
} 